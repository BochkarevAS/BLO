{{ form_start(form, {'action': path('parts_index'), 'method': 'GET', 'attr': {'id': 'form'}}) }}
    {{ form_errors(form) }}

    <div class="row">
        <div class="col-4">
            {{ form_row(form.brand, {'id': 'parts-brand', 'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="col-4">
            {{ form_row(form.model, {'id': 'parts-model', 'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="col-4">
            {{ form_row(form.carcase, {'id': 'parts-carcase', 'attr': {'class': 'form-control'}}) }}
        </div>
    </div>

    <div class="row">
        <div class="col-4">
            {{ form_row(form.company, {'id': 'parts-company', 'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="col-4">
            {{ form_row(form.city, {'id': 'city', 'attr': {'class': 'form-control'}}) }}
        </div>
    </div>

    <div class="row">
        <div class="col-4">
            {{ form_row(form.oem, {'id': 'parts-oem', 'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="col-4">
            {{ form_row(form.engine, {'id': 'parts-engine', 'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="col-4">
            {{ form_row(form.name, {'id': 'parts-name', 'attr': {'class': 'form-control border-primary typeahead'}}) }}
        </div>
    </div>

    <div class="modal-footer">
        <button type="submit" class="btn btn-primary">Найти</button>
    </div>
{{ form_end(form) }}